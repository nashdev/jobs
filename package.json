{
  "name": "nashdev-jobs",
  "version": "0.2.2",
  "description": "NashDev Jobs API + Slack Bot",
  "engines": {
    "node": "8.9.3"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "NODE_ENV=production node ./server/index.js",
    "start:dev": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src":
      "nodemon --exec 'babel-node server/index.js' --ignore client/ --inspect=0.0.0.0:9229",
    "lint": "esw webpack.config.* client tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist",
    "build": "babel-node tools/build.js",
    "test": "jest --config jest.config.json",
    "test:cover":
      "npm run test -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch"
  },
  "author": "egdelwonk",
  "license": "MIT",

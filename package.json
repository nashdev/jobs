{
  "name": "nashdev-jobs",
  "version": "1.0.1",
  "engines": {
    "node": ">=10.9.0",
    "npm": "^6.4.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "build:with-stats": "cross-env NODE_ENV=production webpack --config config/webpack.config.js/client.prod.js --json > build/client/static/bundle-stats.json",
    "start:analyzer": "webpack-bundle-analyzer build/client/static/bundle-stats.json",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "prettier-eslint --list-different \"src/**/*.js\"",
    "lint:css": "stylelint ./src/**/*.css",
    "format": "prettier --write 'src/**/*.*'",
    "postinstall": "npm run build",
    "precommit": "lint-staged",
    "start": "cross-env NODE_ENV=development node scripts/start.js",
    "start:production": "node build/server/server.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:watch": "npm test --verbose -- --watch",
    "test:update": "npm test -- --updateSnapshot",
    "test:ci": "npm test -- --updateSnapshot --ci",
    "setup": "node scripts/setup.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write ./src/**/*.jsx?",
      "git add"
    ],
    "*.css": [
      "prettier-stylelint --write ./src/**/*.css",
      "git add"
    ]
  },
  "moduleRoots": [
    "src/client",
    "src/server",
    "src/shared",
    "src",
    "node_modules"
  ],
  "devDependencies": {
    "autoprefixer": "^9.4.10",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-inline-react-svg": "^1.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",

    "eslint-plugin-security": "^1.4.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-apply": "^0.12.0",
